{
   "manifest_version": 3,
   "name": "Mobile Toolbar For Kiwi Browser",
   "short_name": "Mobile Toolbar",
   "version": "0.1.1",
   "description": "Add a toolbar at the bottom of Chrome Mobile",
   "author": "butaixianran",
   "icons": {
      "128": "icon_128.png"
   },
   "background": {
    "service_worker": "background.js"
   },
  "content_scripts": [{
      "matches": ["<all_urls>"],
      "js": ["content.js"]
   }]
}
